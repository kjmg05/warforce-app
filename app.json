{
  "expo": {
    "name": "WarForce",
    "slug": "WarForce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/wficon.png",
    "splash": {
      "image": "./assets/image/wf_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F01",
      "backgroundImage": "./assets/image/background.jpg"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/wficon.png",
        "backgroundColor": "#0F0F01"
      }
    },
    "web": {
      "favicon": "./assets/image/wficon.png"
    }
  }
}
